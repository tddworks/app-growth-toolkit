{
  "name": "app-growth-toolkit",
  "version": "1.0.0",
  "description": "Curated toolkit of the best tools and resources to help app founders drive revenue and accelerate growth",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all build:css --parallel dev:*",
    "dev:eleventy": "eleventy --serve --watch",
    "dev:css": "tailwindcss -i src/style.css -o _site/css/style.css --watch",
    "build": "npm run build:css && eleventy",
    "build:css": "tailwindcss -i src/style.css -o _site/css/style.css --minify",
    "preview": "eleventy --serve"
  },
  "keywords": [
    "app-growth",
    "app-toolkit",
    "app-founders",
    "app-store",
    "mobile-app",
    "growth-hacking",
    "eleventy",
    "11ty"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.13"
  }
}
